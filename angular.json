{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "signal-ex01": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/signal-ex01",
      "sourceRoot": "projects/signal-ex01/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/signal-ex01",
            "index": "projects/signal-ex01/src/index.html",
            "browser": "projects/signal-ex01/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/signal-ex01/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/signal-ex01/public"
              }
            ],
            "styles": [
              "projects/signal-ex01/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "signal-ex01:build:production"
            },
            "development": {
              "buildTarget": "signal-ex01:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/signal-ex01/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/signal-ex01/public"
              }
            ],
            "styles": [
              "projects/signal-ex01/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
